<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="konkukSW.MP2019.roadline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".UI.SplashActivity"
            android:theme="@style/Splash"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".UI.money.AddMoneyActivity" />
        <activity android:name=".UI.photo.ShowPhotoActivity" />
        <activity android:name=".UI.money.ShowMoneyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.date.ShowDateActivity"></activity>
        <activity android:name=".UI.date.PickDateActivity"></activity>
        <activity android:name=".UI.MainListActivity"></activity>
    </application>

</manifest>